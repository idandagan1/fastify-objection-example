{
  "name": "fastify-objection-example",
  "version": "1.0.0",
  "description": "Simple web application using fastify and objection on top of postgres",
  "main": "src/app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run clean && tsc -p .",
    "clean": "rm -rf ./dist",
    "test": "tap test/**/*.test.js",
    "start": "npm run build && fastify start -l info dist/app.js",
    "docker-compose": "docker-compose --project-directory . --project-name gibush",
    "dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\"  \"tsc -w\" \"fastify start -w -l info -P dist/app.js\"",
    "migrate": "knex migrate:latest"
  },
  "keywords": [
    "pg",
    "fastify",
    "objection"
  ],
  "author": "Idan Dagan",
  "license": "ISC",
  "dependencies": {
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.0.2",
    "fastify-cli": "^2.5.1",
    "fastify-objectionjs": "^0.3.0",
    "fastify-plugin": "^3.0.0",
    "knex": "^0.21.14",
    "objection": "^2.2.3",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "concurrently": "^5.3.0",
    "tap": "^14.0.0",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  }
}
